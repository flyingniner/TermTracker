<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.termtacker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_wgu"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_wgu_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TermActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TermAddEditActivity"
            android:parentActivityName=".TermActivity" />
        <activity
            android:name=".CourseAddEditActivity"
            android:parentActivityName=".CoursesActivity" />
        <activity
            android:name=".CoursesActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".SelectMentorActivity"
            android:parentActivityName=".CourseAddEditActivity" />
        <activity
            android:name=".AssessmentAddEditActivity"
            android:parentActivityName=".AssessmentsActivity" />
        <activity
            android:name=".AssessmentsActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".NotesActivity" />
        <activity android:name=".NoteAddEditActivity"></activity>
    </application>

</manifest>